{
  "expo": {
    "name": "website",
    "slug": "website",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "website",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/FiraSans.ttf"
          ]
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/website"
    }
  }
}
